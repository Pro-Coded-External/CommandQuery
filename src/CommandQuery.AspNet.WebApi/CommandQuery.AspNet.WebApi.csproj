<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <Version>1.0.0</Version>
    <PackageReleaseNotes>
☑️ Removed Help methods
☑️ Returns Error when exception is thrown
☑️ Improved logging
    </PackageReleaseNotes>
    <Authors>Henrik Lau Eriksson</Authors>
    <Description>Command Query Separation for ASP.NET Web API 2 🌐

✔️ Provides generic actions for handling the execution of commands and queries
✔️ Enables APIs based on HTTP POST and GET
📄 https://hlaueriksson.me/CommandQuery.AspNet.WebApi/
    </Description>
    <PackageId>CommandQuery.AspNet.WebApi</PackageId>
    <PackageProjectUrl>https://github.com/hlaueriksson/CommandQuery</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <PackageTags>CommandQuery;Command;Query;CQS;WebApi;AspNetWebApi</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/hlaueriksson/CommandQuery.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\icon.png" Link="icon.png" Pack="true" PackagePath="\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.WebApi" Version="5.2.7" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CommandQuery.Extensions\CommandQuery.Extensions.csproj" />
    <ProjectReference Include="..\CommandQuery\CommandQuery.csproj" />
    <ProjectReference Include="..\CommandQuery.DependencyInjection\CommandQuery.DependencyInjection.csproj" />
  </ItemGroup>

</Project>