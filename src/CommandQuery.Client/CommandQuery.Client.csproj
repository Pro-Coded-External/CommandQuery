<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Nullable>enable</Nullable>
    <Version>1.0.0</Version>
    <PackageReleaseNotes>
☑️ Added CommandQueryException
  ☑️ This is thrown when IsSuccessStatusCode is false
  ☑️ Provides access to the Error from the API
    </PackageReleaseNotes>
    <Authors>Henrik Lau Eriksson</Authors>
    <Description>Clients for CommandQuery

✔️ CommandClient
✔️ QueryClient
📄 https://hlaueriksson.me/CommandQuery.Client/
    </Description>
    <PackageId>CommandQuery.Client</PackageId>
    <PackageProjectUrl>https://github.com/hlaueriksson/CommandQuery</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <PackageTags>CommandQuery;Command;Query;CQS</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/hlaueriksson/CommandQuery.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\icon.png" Link="icon.png" Pack="true" PackagePath="\" />
  </ItemGroup>

  <Import Project="../../Analyzers.props" />

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="5.2.7" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CommandQuery.Abstractions\CommandQuery.Abstractions.csproj" />
  </ItemGroup>

</Project>